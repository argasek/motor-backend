{
  "name": "motor-cms/motor-backend",
  "description": "Motor backend package",
  "license": "MIT",
  "keywords": [
    "motor"
  ],
  "authors": [
    {
      "name": "Reza Esmaili",
      "email": "me@dfox.info"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "^7.0",
    "laravel/tinker": "^2.0",
    "motor-cms/motor-core": "0.10.*",
    "wpb/string-blade-compiler": "dev-master",
    "doctrine/dbal": "^2.10",
    "php-console/php-console": "^3.1",
    "guzzlehttp/guzzle": "^6.5",
    "barryvdh/laravel-ide-helper": "^2.7",
    "astrotomic/laravel-translatable": "^11.8",
    "laracasts/flash": "^3.0",
    "laravelcollective/html": "^6.1",
    "spatie/laravel-fractal": "^5.7",
    "intervention/imagecache": "^2.4",
    "league/glide-laravel": "^1.0",
    "lavary/laravel-menu": "^1.7",
    "nstapelbroek/culpa": "dev-master",
    "symfony/intl": "^5.0",
    "kris/laravel-form-builder": "^1.30",
    "spatie/laravel-medialibrary": "^7.19",
    "spatie/laravel-permission": "^3.0",
    "kalnoy/nestedset": "^5.0",
    "martinlindhe/laravel-vue-i18n-generator": "^0.1",
    "tightenco/ziggy": "^0.9",
    "laravel/horizon": "^4.0",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "facade/ignition": "^2.0",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^4.1",
    "phpunit/phpunit": "^8.5",
    "symfony/css-selector": "^5.0",
    "symfony/dom-crawler": "^5.0",
    "laravel/dusk": "^5.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jmarcher/culpa-laravel-5"
    },
    {
      "type": "vcs",
      "url": "https://github.com/iNtergrated/StringBladeCompiler"
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Motor\\Backend\\": "src",
      "Tests\\Motor\\Backend\\": "tests"
    },
    "files": [
      "helpers/motor_backend_helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motor\\Backend\\Providers\\MotorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
